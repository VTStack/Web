export declare type ValueType = string | string[] | number | boolean | Date;
export declare type QueryStateValue = string | string[];
export declare type QueryStateResetValue = null | undefined;
export declare type QueryState = Record<string, QueryStateValue>;
export declare type QueryStateMerge = Record<string, QueryStateValue | QueryStateResetValue>;
export declare const EMPTY_ARRAY_STRING = "[\u00A0]";
export declare function stripLeadingHashOrQuestionMark(s?: string): string;
export declare function parseQueryState(queryString: string): QueryState | null;
export declare function createMergedQuery(...queryStates: QueryStateMerge[]): string;
export declare function toQueryStateValue(value: ValueType | unknown): QueryStateValue | null;
export declare const newStringArray: () => string[];
export declare function parseQueryStateValue<T extends ValueType>(value: QueryStateValue, defaultValue: T): T | null;
